     /**
     * Define the shortcuts data section without any profile to have all shortcuts cleared after new menu items have been updated.
     */
    shortcuts = [ null ]

     /**
     * Define the mode available layouts, the first layout will act as the default one.
     */
    layouts = [
        "Packages/com.unity.tiny/Unity.Editor/Unity.Editor.Modes/Resources/Layouts/Editor.wlt"
    ]

     /**
     * Define the capabilities of the mode. Thing that this mode supports and do not support.
     */
    capabilities = {
        layers = false
        playbar = true
    }

     /**
     * Define the list of window classes that will show up when expanding the available tabs contextual menu of a dock area.
     */
    pane_types = [
        "SceneView"
        "Game"
        "InspectorWindow"
        "EntityHierarchyWindow"
        "ProjectBrowser"
        "ProfilerWindow"
    ]

     /**
     * Define custom behaviors the mode supports and acted on.
     */
    execute_handlers = {
        gui_playbar = "DOTS/GUI/PlayBar"
        gui_pause = "DOTS/Pause"
    }
}
